<div class="container">
  <div class="row">
    <div class="col-12">
      <%= simple_form_for [@list, @bookmark] do |f| %>
        <%= f.input :comment %>
        <%= f.association :list %>
        <%= f.association :movie %>
        <%= f.submit "Add movie", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
